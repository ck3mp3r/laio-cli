{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "repositories": [
    "ck3mp3r/rmx-cli"
  ],
  "addLabels": [
    "renovatebot-pr"
  ],
  "automerge": true,
  "automergeType": "branch",
  "automergeSchedule": [
    "at any time"
  ],
  "dependencyDashboard": true,
  "onboarding": false,
  "prConcurrentLimit": 0,
  "prHourlyLimit": 0,
  "packageRules": [
    {
      "groupName": "nix flake dependencies",
      "groupSlug": "nix-flake",
      "matchManagers": [
        "nix"
      ],
      "automerge": true,
      "enabled": true
    }
  ],
  "lockFileMaintenance": {
    "enabled": true,
    "automerge": true,
    "schedule": [
      "at any time"
    ]
  },
  "schedule": [
    "at any time"
  ],
  "nix": {
    "enabled": true
  }
}
